{
    "name": "nickdekruijk/pageviews",
    "description": "Track pageviews in your Laravel app",
    "keywords": [
        "nickdekruijk",
        "pageviews"
    ],
    "homepage": "https://github.com/nickdekruijk/pageviews",
    "license": "MIT",
    "authors": [
        {
            "name": "Nick de Kruijk",
            "email": "git@nickdekruijk.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "illuminate/support": "5.7.*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "NickDeKruijk\\Pageviews\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NickDeKruijk\\Pageviews\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NickDeKruijk\\Pageviews\\PageviewsServiceProvider"
            ],
            "aliases": {
                "Skeleton": "NickDeKruijk\Pageviews\\PageviewsFacade"
            }
        }
    }
}
